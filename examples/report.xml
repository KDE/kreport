<report:content xmlns:report="http://kexi-project.org/report/2.0"
                xmlns:fo="urn:oasis:names:tc:opendocument:xmlns:xsl-fo-compatible:1.0"
                xmlns:svg="urn:oasis:names:tc:opendocument:xmlns:svg-compatible:1.0">
    <report:title>Report</report:title>
    <report:grid report:grid-divisions="4" report:grid-snap="1" report:page-unit="cm" report:grid-visible="1" />
    <report:page-style report:print-orientation="portrait"
                       fo:margin-bottom="1.5cm" fo:margin-top="2.0cm"
                       fo:margin-left="3.0cm" fo:margin-right="4.0cm"
                       report:page-size="A5" >predefined</report:page-style>
    <report:body>
        <report:detail>
            <report:section svg:height="5.0cm" fo:background-color="#ffffff" report:section-type="detail"/>
        </report:detail>
    </report:body>
</report:content>
